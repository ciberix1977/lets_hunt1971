{
  "version": 2,
  "builds": [
    {
      "src": "api/register.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/register",
      "dest": "api/register.js"
    },
    {
      "src": "/(.*)",
      "headers": {
        "cache-control": "public, max-age=3600"
      },
      "dest": "/$1"
    }
  ]
}